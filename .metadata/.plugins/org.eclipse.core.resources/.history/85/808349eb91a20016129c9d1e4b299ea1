		Parallelogram testParallelogram= new Parallelogram(new Point(5.0,5.0), new Point(5.0,5.0),new Point(5.0,5.0),new Point(5.0,5.0)); 
		
		assertEquals("area must be 100", 100, testParallelogram.getArea());
	}

